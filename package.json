    "lint:javascript": "./node_modules/.bin/eslint ./src ./server/server.js",
    "lint:stylelint": "./node_modules/.bin/stylelint './src/**/*.scss' --syntax scss",
    "lint": "npm run lint:javascript && npm run lint:stylelint",
    "start": "NODE_ENV='development' nodemon ./server/server.js --exec babel-node --inspect",
    "start:prod": "NODE_ENV='production' node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
